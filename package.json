{
  "name": "jscrewit",
  "version": "2.13.0",
  "description": "Converts plain JavaScript into JSFuck code, which consists of only six different characters: ! ( ) + [ ]",
  "keywords": [
    "esoteric",
    "jsfuck",
    "obfuscation"
  ],
  "homepage": "https://jscrew.it",
  "bugs": "https://github.com/fasttime/JScrewIt/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "Features.md",
    "JScrewIt.html",
    "doc",
    "lib",
    "output.txt",
    "screw.js",
    "tools",
    "ui",
    "!.*"
  ],
  "main": "lib/jscrewit.js",
  "types": "lib/jscrewit.d.ts",
  "bin": "screw.js",
  "directories": {
    "lib": "lib",
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/JScrewIt.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "docs": "gulp docs",
    "feature-info": "build/legacy/feature-info.js",
    "test": "mocha --check-leaks --require=ebdd --ui=ebdd test/{*,*/*}.spec.js"
  },
  "devDependencies": {
    "@fasttime/gulp-lint": "~0.25.0",
    "acorn": "~7.1.1",
    "art-js": "~0.9.2",
    "chalk": "~4.0.0",
    "del": "~5.1.0",
    "ebdd": "~0.4.0",
    "eslint-plugin-ebdd": "~0.1.1",
    "expectations": "~1.0.0",
    "gulp": "~4.0.2",
    "gulp-jsdoc3": "~3.0.0",
    "gulp-rename": "~2.0.0",
    "gulp-tap": "2.0.0",
    "gulp-typedoc": "~2.2.5",
    "gulp-uglify": "~3.0.2",
    "handlebars": "~4.7.6",
    "mocha": "~3.5.3",
    "mocha-bar": "~0.1.0",
    "nyc": "~15.0.1",
    "postrequire": "~0.1.1",
    "rollup": "~2.3.3",
    "rollup-plugin-cleanup": "~3.1.1",
    "typedoc": "~0.17.3",
    "typedoc-plugin-markdown": "~2.2.17"
  }
}
